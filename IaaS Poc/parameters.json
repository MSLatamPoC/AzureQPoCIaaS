{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "networkInterfaceName1": {
            "value": "vm-front634_z1"
        },
        "networkSecurityGroupName": {
            "value": "VM-FRONT-nsg"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "RDP",
                    "properties": {
                        "priority": 300,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "3389"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "SUBNET-BACKEND"
        },
        "virtualNetworkName": {
            "value": "VNET-POC"
        },
        "addressPrefixes": {
            "value": [
                "192.168.0.0/16"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "SUBNET-FRONT",
                    "properties": {
                        "addressPrefix": "192.168.10.0/24"
                    }
                },
                {
                    "name": "SUBNET-BACKEND",
                    "properties": {
                        "addressPrefix": "192.168.20.0/24"
                    }
                }
            ]
        },
        "publicIpAddressName1": {
            "value": "VM-FRONT-ip"
        },
        "publicIpAddressType": {
            "value": "Static"
        },
        "publicIpAddressSku": {
            "value": "Standard"
        },
        "pipDeleteOption": {
            "value": "Detach"
        },
        "virtualMachineName": {
            "value": "VM-FRONT"
        },
        "virtualMachineName1": {
            "value": "VM-FRONT"
        },
        "virtualMachineComputerName1": {
            "value": "VM-FRONT"
        },
        "virtualMachineRG": {
            "value": "RG-POCSIMPLE"
        },
        "osDiskType": {
            "value": "Standard_LRS"
        },
        "osDiskDeleteOption": {
            "value": "Delete"
        },
        "virtualMachineSize": {
            "value": "Standard_B1s"
        },
        "nicDeleteOption": {
            "value": "Detach"
        },
        "hibernationEnabled": {
            "value": false
        },
        "adminUsername": {
            "value": "Adminpoc"
        },
        "adminPassword": {
            "value": null
        },
        "patchMode": {
            "value": "AutomaticByOS"
        },
        "enableHotpatching": {
            "value": false
        },
        "securityType": {
            "value": "TrustedLaunch"
        },
        "secureBoot": {
            "value": true
        },
        "vTPM": {
            "value": true
        },
        "virtualMachine1Zone": {
            "value": "1"
        },
        "autoShutdownStatus": {
            "value": "Enabled"
        },
        "autoShutdownTime": {
            "value": "20:00"
        },
        "autoShutdownTimeZone": {
            "value": "SA Pacific Standard Time"
        },
        "autoShutdownNotificationStatus": {
            "value": "Enabled"
        },
        "autoShutdownNotificationLocale": {
            "value": "en"
        },
        "autoShutdownNotificationEmail": {
            "value": "haamos@outlook.com"
        }
    }
}